  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "prebbuild-lib": "npm run copyright",
    "build-lib": "rm -rf ./dist && tsc --project build.tsconfig.json && tscpaths -p build.tsconfig.json -s ./src -o ./dist && copyfiles -V -u 1 ./src/**/*.scss ./dist",

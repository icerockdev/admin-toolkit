    "@types/react-router-dom": "^5.1.7",
    "@types/throttle-debounce": "^2.1.0",
    "babel-preset-mobx": "^2.0.0",
    "copyfiles": "^2.4.1",
    "copyright-header": "^0.4.6",
    "craco": "^0.0.3",
    "craco-alias": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
